<Window x:Class="ahp.WindowSelectInconsistentElements"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ahp"
        mc:Ignorable="d"
        Title="Identify Potential Inconsistent Elements by the Bias Identifying Vector" Height="300" Width="300" WindowStartupLocation="CenterScreen" >
        <Grid Margin="20">
            <Grid.RowDefinitions>
                <RowDefinition Height="*"/>
                <RowDefinition Height="Auto"/>
            </Grid.RowDefinitions>

        <GroupBox Header="Select inconsistent potentials" Margin="0,0,0,10" Background="#FFE9F0F7">
                <Grid Name="GrdOptions"></Grid>
            </GroupBox>
            <Button Content="OK" Name="BtnOK" Click="BtnOK_Click" Grid.Row="1" HorizontalAlignment="Right" Padding="10,5"/>

        </Grid>
</Window>
